{
  "name": "aftr-contracts",
  "version": "1.0.0",
  "description": "AFTR smart contracts for the Arweave ecosystem",
  "main": "index.js",
  "scripts": {
    "init": "node -r dotenv/config ./tests/init.js",
    "init-play": "node -r dotenv/config ./tests/init-play.js",
    "fcp": "node -r dotenv/config ./tests/fcp.js",
    "fcp-test": "node -r dotenv/config ./tests/fcp-test.js",
    "test": "node -r dotenv/config ./tests/test.js",
    "test-script": "node -r dotenv/config ./tests/test-script.js",
    "test-create": "node -r dotenv/config ./tests/test-create.js",
    "sw-read": "node -r dotenv/config ./tests/sw-read.js",
    "warp-test": "node -r dotenv/config ./tests/warp-test.js",
    "genkey": "node -r dotenv/config ./tests/genkey.js",
    "test-inc": "node -r dotenv/config ./tests/test-inc.js",
    "test-vote": "node -r dotenv/config ./tests/test-vote.js",
    "test-input": "node -r dotenv/config ./tests/test-input.js",
    "test-dep": "node -r dotenv/config ./tests/test-dep.js",
    "build-contract": "node ./scripts/build.js",
    "build-contract-tests": "node ./scripts/build-tests.js",
    "contract-test": "node ./scripts/build.js && node ./build/vehicle/contract-tests.js",
    "tests": "node ./build/vehicle/contract-new-tests.js",
    "jest": "jest --detectOpenHandles --testPathPattern=./__tests__/contract.test.ts",
    "joe-test": "node -r dotenv/config ./tests/joe-test.js"
  },
