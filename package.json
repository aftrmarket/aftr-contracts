    "esm": "^3.2.25",
    "if-env": "^1.0.4",
    "smartweave": "^0.4.49",
    "verto-cache-interface": "^1.2.4",
    "warp-contracts": "^1.2.13"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "arweave-jest-fuzzing": "^1.0.1",
    "babel-jest": "^29.2.1",
    "dotenv": "^10.0.0",
